{
	"manifest_version": 2,
	"name": "__MSG_extensionName__",
	"description": "__MSG_extensionDescription__",
	"version": "1.0",
	"homepage_url": "https://github.com/mozillabrasil/sumo_live_helper",
	"default_locale": "en",
	"author": "Mozilla Brasil",
	"applications": {
		"gecko": {
			"strict_min_version": "58.0",
			"id": "sumolivehelper@mozillabr.org"
		}
	},
	"icons": {
		"64": "/res/icons/icon.png"
	},
	"browser_action": {
		"browser_style": true,
		"default_icon": {
			"16": "/res/icons/icon-16.png",
			"32": "/res/icons/icon-32.png"
		}
	},
	"background": {
		"scripts": ["/js/background.js"]
	},
	"browser_action": {
		"browser_style": true,
		"default_icon": {
			"16": "../res/icons/icon-16.png",
			"32": "../res/icons/icon-32.png"
		},
		"default_title": "__MSG_extensionName__",
		"default_popup": "../html/popup.html"
	},
	"background": {
		"page": "/html/popup.html"
	},
	"options_ui": {
		"browser_style": false,
		"open_in_tab": true,
		"page": "html/preferences.html"
	},
	"permissions": [
		"tabs",
		"browserSettings",
		"activeTab"
	]
}
